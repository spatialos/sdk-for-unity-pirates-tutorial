{
  "build": {
    "tasks_filename": "spatialos.unity.worker.build.json",
    "generated_build_scripts_type": "unity"
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "physics"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 200
      }
    },
    "streaming_query": [
      {
        "bounded_component_streaming_query": {
          "component_name": "improbable.terrain.Terrain",
          "radius": 2000
        }
      }
    ],
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkoutAllInitially": true
    }
  },
  "managed": {
    "linux": {
      "artifact_name": "UnityWorker@Linux.zip",
      "command": "UnityWorker@Linux",
      "arguments": [
        "+projectName",
        "${IMPROBABLE_PROJECT_NAME}",
        "+receptionistHost",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+workerType",
        "${IMPROBABLE_WORKER_NAME}",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+useExternalIpForBridge",
        "true",
        "+infraServicesUrl",
        "${IMPROBABLE_API_URL}",
        "+assemblyName",
        "${IMPROBABLE_ASSEMBLY_NAME}",
        "+linkProtocol",
        "RakNet",
        "-logfile",
        "${IMPROBABLE_LOG_FILE}",
        "-batchmode",
        "-nographics"
      ]
    },
    "windows": {
      "artifact_name": "UnityWorker@Windows.zip",
      "command": "UnityWorker@Windows.exe",
      "arguments": [
        "+projectName",
        "${IMPROBABLE_PROJECT_NAME}",
        "+receptionistIp",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+workerType",
        "${IMPROBABLE_WORKER_NAME}",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+useExternalIpForBridge",
        "true",
        "+infraServicesUrl",
        "${IMPROBABLE_API_URL}",
        "+assemblyName",
        "${IMPROBABLE_ASSEMBLY_NAME}",
        "+linkProtocol",
        "RakNet",
        "-logfile",
        "${IMPROBABLE_LOG_FILE}",
        "-batchmode",
        "-nographics"
      ]
    },
    "macos": {
      "artifact_name": "UnityWorker@Mac.zip",
      "command": "UnityWorker@Mac.app/Contents/MacOS/UnityWorker@Mac",
      "arguments": [
        "+projectName",
        "${IMPROBABLE_PROJECT_NAME}",
        "+receptionistIp",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+workerType",
        "${IMPROBABLE_WORKER_NAME}",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+useExternalIpForBridge",
        "true",
        "+infraServicesUrl",
        "${IMPROBABLE_API_URL}",
        "+assemblyName",
        "${IMPROBABLE_ASSEMBLY_NAME}",
        "+linkProtocol",
        "RakNet",
        "-logfile",
        "${IMPROBABLE_LOG_FILE}",
        "-batchmode",
        "-nographics"
      ]
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE",
      "windows": {
        "command": "build/worker/UnityWorker@Windows/UnityWorker@Windows.exe",
        "arguments": [
          "+assemblyName",
          "local_assembly",
          "+workerType",
          "UnityWorker",
          "+infraServicesUrl",
          "http://127.0.0.1:21000",
          "+projectName",
          "${IMPROBABLE_PROJECT_NAME}",
          "-batchmode",
          "-nographics",
          "-logfile",
          "external-default-unityworker.log"
        ]
      },
      "macos": {
        "command": "open",
        "arguments": [
          "./build/worker/UnityWorker@Mac/UnityWorker@Mac.app",
          "--args",
          "+assemblyName",
          "local_assembly",
          "+workerType",
          "UnityWorker",
          "+infraServicesUrl",
          "http://127.0.0.1:21000",
          "+projectName",
          "${IMPROBABLE_PROJECT_NAME}",
          "-batchmode",
          "-nographics",
          "-logfile",
          "external-default-unityworker.log"
        ]
      }
    }
  }
}
